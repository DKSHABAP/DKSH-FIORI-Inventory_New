<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog title="{i18n>PlantTitle}" liveChange="onLiveChangePlant" multiSelect="true" confirm="onConfirmChangePlant"
		cancel="onCancelValueHelp" items="{plantModel>/results}" class="sapUiSizeCompact">
		<StandardListItem title="{parts:[{path:'plantModel>plantName'},{path:'plantModel>plant'}],formatter:'.formatter.f4ValueBind'}"
			tooltip="{parts:[{path:'plantModel>Plantname'},{path:'plantModel>Plant'}],formatter:'.formatter.f4ValueBind'}" type="Active"/>
	</SelectDialog>
</core:FragmentDefinition>